{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@components/*": [
                "src/components/*"
            ],
            "@layouts/*": [
                "src/layouts/*"
            ],
            "@logs/*": [
                "src/logs/*"
            ],
            "@pages/*": [
                "src/pages/*"
            ],
            "@public/*": [
                "src/public/*"
            ],
            "@reducers/*": [
                "src/reducers/*"
            ],
            "@services/*": [
                "src/services/*"
            ],
            "@store/*": [
                "src/store/*"
            ],
            "@style/*": [
                "src/style/*"
            ],
            "@/*": [
                "src/*"
            ],
            "@validations/*": [
                "src/validations/*"
            ],
            "@Login/*": [
                "src/pages/Login/*"
            ]
        }
    }
}